[user]
        name = <%= name %>
        email = <%= email %>

[core]
        autocrlf = false
        safecrlf = true

[alias]
        co = checkout
        ci = commit
        st = status
        br = branch
        hist = log --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=short
        type = cat-file -t
        dump = cat-file -p

[color]
        diff = "auto"
        status = "auto"
        branch = "auto"